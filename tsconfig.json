{
  "compilerOptions": {
    "alwaysStrict": true,
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ES2020"],
    
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "cdk.out",
    "standalone/out"
  ]
}